{
  "name": "northwind-odata-v4-demo",
  "version": "0.0.1",
  "scripts": {
    "start": "nodemon --watch src --watch conf --ext ts,json,config.js --exec ts-node src/server.ts",
    "start:prod": "node lib/server.js",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "build": "tsc",
    "lint": "eslint src/** --fix",
    "release": "standard-version --no-verify && git push --follow-tags origin master",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "bin": {
    "odata-service": "./lib/server.js"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@newdash/newdash": "^5.18.0",
    "@odata/server": "^0.2.48",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "reflect-metadata": "^0.1.13",
    "sql.js": "^1.5.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "commitlint": "^11.0.0",
    "eslint": "^7.28.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "nodemon": "^2.0.12",
    "standard-version": "^9.3.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.2"
  },
  "license": "UNLICENSED"
}
